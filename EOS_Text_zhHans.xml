		<!--龙脉长歌·上-->
		<Replace Tag="LOC_PROJECT_MELODY_OF_STARSONG_ACT1_NAME" Language="zh_Hans_CN">
			<Text>龙脉长歌上部</Text>
		</Replace>
		<Replace Tag="LOC_PROJECT_MELODY_OF_STARSONG_ACT1_SHORT_NAME" Language="zh_Hans_CN">
